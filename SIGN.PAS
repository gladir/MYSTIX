Var
 Year,Month,Day:Integer;
 I:Integer;
 Err:Word;
 Ok:Boolean;

Function StrToUpper(S:String):String;
Var
 I:Byte;
Begin
 For I:=1 to Length(S)do Begin
  If S[I] in['a'..'z']Then S[I]:=Chr(Ord(S[I])-32);
 End;
 StrToUpper:=S;
End;

BEGIN
 If(ParamStr(1)='/?')or(ParamStr(1)='--help')or(ParamStr(1)='-h')or
   (ParamStr(1)='/h')or(ParamStr(1)='/H')Then Begin
  WriteLn('SIGN : Cette commande permet d''afficher ',
                      'le signe astrologique.');
  WriteLn;
  WriteLn('Syntaxe : SIGN annee mois jour');
 End
  Else
 If ParamCount>0Then Begin
  Val(ParamStr(1),Year,Err);
  If Err>0 Then Begin
   WriteLn('Format d''ann‚e invalide');
   Halt;
  End;
  Month:=-1;
  For I:=Low(Mon) to High(Mon)do Begin
   If StrToUpper(ParamStr(2))=StrToUpper(Mon[I])Then Begin
    Month:=I;
    Break;
   End;
  End;
  If Month=-1 Then Begin
   Val(ParamStr(2),Month,Err);
   If Err>0 Then Begin
    WriteLn('Format de mois invalide');
    Halt;
   End;
  End;
  Val(ParamStr(3),Day,Err);
  If Err>0 Then Begin
   WriteLn('Format de jour invalide');
   Halt;
  End;
  For I:=Low(Signs) to High(Signs)do Begin
   If(Month=Signs[I].BeginMonth)and(Day>=Signs[I].BeginDay)or
     (Month=Signs[I].EndMonth)and(Day<=Signs[I].EndDay)Then Begin
    WriteLn('Signe de zodiaque : ',Signs[I].NomSign);
    WriteLn('Element : ',Signs[I].Element);
    Break;
   End;
  End;
  For I:=Low(SignsChinois) to High(SignsChinois)do Begin
   Ok:=(Year=SignsChinois[I].BeginYear)and(Month>=SignsChinois[I].BeginMonth);
   If(Ok)Then Begin
    If(Month=SignsChinois[I].BeginMonth)Then Ok:=Ok and(Day>=SignsChinois[I].BeginDay);
   End;
   If(((Year=SignsChinois[I].EndYear)and(Month<=SignsChinois[I].EndMonth)))Then Begin
    If(Month=SignsChinois[I].EndMonth)Then Ok:=(Day<=SignsChinois[I].EndDay);
   End;
   If(Ok)Then Begin
    WriteLn('Signe chinois : ',SignsChinois[I].NomSign);
    Break;
   End;
  End;
 End;
END.